<?xml version="1.0" encoding="utf-8"?>
<OfxImageEffectResource>
<OfxPlugin name="de.magix:Colorization">

	<OfxResourceSet ofxHost="default">
		<OfxPropLabel>Coloración VEGAS</OfxPropLabel>

		<OfxImageEffectPluginPropGrouping>VEGAS\IA</OfxImageEffectPluginPropGrouping>
		<OfxPropPluginDescription>Colorea material en blanco y negro.</OfxPropPluginDescription>
		<OfxImageEffectContext name="default">
		</OfxImageEffectContext>

		<OfxImageEffectContext name="OfxImageEffectContextFilter">
			
			<OfxMessage name="ModelNotFound">Este plugin requiere datos adicionales. Por favor, instale el pack promocional VEGAS IA; puede descargarlo en www.vegascreativesoftware.com.</OfxMessage>
			
			<OfxParamTypeChoice name="Localization">
				<OfxParamPropChoiceOption>Este plugin requiere datos adicionales. Por favor, instale el pack promocional VEGAS IA; puede descargarlo en www.vegascreativesoftware.com.</OfxParamPropChoiceOption>
			</OfxParamTypeChoice>
			
			<OfxParamTypeChoice name="Preprocess">
				<OfxParamPropChoiceOption>Ninguno</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Solo estimación (no aplicar a la exportación)</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Aplicar a la exportación</OfxParamPropChoiceOption>
				<OfxPropLabel>Optimización de luminancia</OfxPropLabel>
				<OfxPropShortLabel>Optimización de luminancia</OfxPropShortLabel>
				<OfxPropLongLabel>Optimización de luminancia</OfxPropLongLabel>
				<OfxParamPropHint>Preprocesamiento para optimizar la luminancia.</OfxParamPropHint>
			</OfxParamTypeChoice>

			<OfxParamTypeChoice name="Precision">
				<OfxParamPropChoiceOption>Baja</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Medio</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Alta</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Ajustar al tamaño de entrada</OfxParamPropChoiceOption>
				<OfxPropLabel>Precisión</OfxPropLabel>
				<OfxPropShortLabel>Precisión</OfxPropShortLabel>
				<OfxPropLongLabel>Precisión</OfxPropLongLabel>
				<OfxParamPropHint>Equilibrio entre precisión y velocidad.</OfxParamPropHint>
			</OfxParamTypeChoice>

			<OfxParamTypeBoolean name="InvertColorA">
				<OfxPropLabel>Invertir verde y rojo</OfxPropLabel>
				<OfxPropShortLabel>Invertir verde y rojo</OfxPropShortLabel>
				<OfxPropLongLabel>Invertir verde y rojo</OfxPropLongLabel>
				<OfxParamPropHint>Invierte el canal &apos;a&apos; (verde y rojo).</OfxParamPropHint>
			</OfxParamTypeBoolean>
			
			<OfxParamTypeBoolean name="InvertColorB">
				<OfxPropLabel>Invertir azul y amarillo</OfxPropLabel>
				<OfxPropShortLabel>Invertir azul y amarillo</OfxPropShortLabel>
				<OfxPropLongLabel>Invertir azul y amarillo</OfxPropLongLabel>
				<OfxParamPropHint>Invierte el canal &apos;b&apos; (azul y amarillo).</OfxParamPropHint>
			</OfxParamTypeBoolean>

			<OfxParamTypeDouble name="ColorShiftA">
				<OfxPropLabel>Alternar entre verde y rojo</OfxPropLabel>
				<OfxPropShortLabel>Alternar entre verde y rojo</OfxPropShortLabel>
				<OfxPropLongLabel>Alternar entre verde y rojo</OfxPropLongLabel>
				<OfxParamPropHint>Cambia el centro de color al canal &apos;a&apos; (verde y rojo).</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeDouble name="ColorShiftB">
				<OfxPropLabel>Alternar entre azul y amarillo</OfxPropLabel>
				<OfxPropShortLabel>Alternar entre azul y amarillo</OfxPropShortLabel>
				<OfxPropLongLabel>Alternar entre azul y amarillo</OfxPropLongLabel>
				<OfxParamPropHint>Cambia el centro de color al canal &apos;b&apos; (azul y amarillo).</OfxParamPropHint>
			</OfxParamTypeDouble>

			<OfxParamTypeDouble name="ColorMagnificationA">
				<OfxPropLabel>Ganancia de verde y rojo</OfxPropLabel>
				<OfxPropShortLabel>Ganancia de verde y rojo</OfxPropShortLabel>
				<OfxPropLongLabel>Ganancia de verde y rojo</OfxPropLongLabel>
				<OfxParamPropHint>Ganancia en canal &apos;a&apos; (verde y rojo).</OfxParamPropHint>
			</OfxParamTypeDouble>

			<OfxParamTypeDouble name="ColorMagnificationB">
				<OfxPropLabel>Ganancia de azul y amarillo</OfxPropLabel>
				<OfxPropShortLabel>Ganancia de azul y amarillo</OfxPropShortLabel>
				<OfxPropLongLabel>Ganancia de azul y amarillo</OfxPropLongLabel>
				<OfxParamPropHint>Ganancial en canal &apos;a&apos; (azul y amarillo).</OfxParamPropHint>
			</OfxParamTypeDouble>
			
		</OfxImageEffectContext>
	</OfxResourceSet>
</OfxPlugin>
