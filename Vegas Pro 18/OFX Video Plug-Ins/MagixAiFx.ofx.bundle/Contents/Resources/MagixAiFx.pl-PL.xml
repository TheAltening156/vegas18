<?xml version="1.0" encoding="utf-8"?>
<OfxImageEffectResource>
<OfxPlugin name="de.magix:Colorization">

	<OfxResourceSet ofxHost="default">
		<OfxPropLabel>VEGAS Koloryzacja</OfxPropLabel>

		<OfxImageEffectPluginPropGrouping>VEGAS\Ai</OfxImageEffectPluginPropGrouping>
		<OfxPropPluginDescription>Koloryzuje nagrania czarno-białe.</OfxPropPluginDescription>
		<OfxImageEffectContext name="default">
		</OfxImageEffectContext>

		<OfxImageEffectContext name="OfxImageEffectContextFilter">
			
			<OfxMessage name="ModelNotFound">Ta wtyczka wymaga dodatkowych danych. Zainstaluj pakiet VEGAS Ai; możesz go pobrać ze strony www.vegascreativesoftware.com.</OfxMessage>
			
			<OfxParamTypeChoice name="Localization">
				<OfxParamPropChoiceOption>Ta wtyczka wymaga dodatkowych danych. Zainstaluj pakiet VEGAS Ai; możesz go pobrać ze strony www.vegascreativesoftware.com.</OfxParamPropChoiceOption>
			</OfxParamTypeChoice>
			
			<OfxParamTypeChoice name="Preprocess">
				<OfxParamPropChoiceOption>Brak</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Tylko szacowanie (nie dotyczy przesyłania)</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Zezwól na przesyłanie</OfxParamPropChoiceOption>
				<OfxPropLabel>Dopasuj luminancję</OfxPropLabel>
				<OfxPropShortLabel>Dopasuj luminancję</OfxPropShortLabel>
				<OfxPropLongLabel>Dopasuj luminancję</OfxPropLongLabel>
				<OfxParamPropHint>Wstępna obróbka w celu dopasowania luminancji.</OfxParamPropHint>
			</OfxParamTypeChoice>

			<OfxParamTypeChoice name="Precision">
				<OfxParamPropChoiceOption>Niska</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Średnia</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Wysoka</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Dopasuj rozmiar wejścia</OfxParamPropChoiceOption>
				<OfxPropLabel>Precyzja</OfxPropLabel>
				<OfxPropShortLabel>Precyzja</OfxPropShortLabel>
				<OfxPropLongLabel>Precyzja</OfxPropLongLabel>
				<OfxParamPropHint>Kompromis pomiędzy dokładnością a szybkością.</OfxParamPropHint>
			</OfxParamTypeChoice>

			<OfxParamTypeBoolean name="InvertColorA">
				<OfxPropLabel>Odwróć zielony i czerwony</OfxPropLabel>
				<OfxPropShortLabel>Odwróć zielony i czerwony</OfxPropShortLabel>
				<OfxPropLongLabel>Odwróć zielony i czerwony</OfxPropLongLabel>
				<OfxParamPropHint>Odwraca kanał &apos;a&apos; (zielony i czerwony)</OfxParamPropHint>
			</OfxParamTypeBoolean>
			
			<OfxParamTypeBoolean name="InvertColorB">
				<OfxPropLabel>Odwróć niebieski i żółty</OfxPropLabel>
				<OfxPropShortLabel>Odwróć niebieski i żółty</OfxPropShortLabel>
				<OfxPropLongLabel>Odwróć niebieski i żółty</OfxPropLongLabel>
				<OfxParamPropHint>Odwraca kanał &apos;b&apos; (niebieski i żółty)</OfxParamPropHint>
			</OfxParamTypeBoolean>

			<OfxParamTypeDouble name="ColorShiftA">
				<OfxPropLabel>Zmień między zielonym i czerwonym</OfxPropLabel>
				<OfxPropShortLabel>Zmień między zielonym i czerwonym</OfxPropShortLabel>
				<OfxPropLongLabel>Zmień między zielonym i czerwonym</OfxPropLongLabel>
				<OfxParamPropHint>Przesuń środek koloru na kanale &apos;a&apos; (zielony i czerwony).</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeDouble name="ColorShiftB">
				<OfxPropLabel>Przesuń pomiędzy niebieskim i żółtym</OfxPropLabel>
				<OfxPropShortLabel>Przesuń pomiędzy niebieskim i żółtym</OfxPropShortLabel>
				<OfxPropLongLabel>Przesuń pomiędzy niebieskim i żółtym</OfxPropLongLabel>
				<OfxParamPropHint>Przesuń środek koloru na kanale &apos;b&apos; (niebieski i żółty).</OfxParamPropHint>
			</OfxParamTypeDouble>

			<OfxParamTypeDouble name="ColorMagnificationA">
				<OfxPropLabel>Zwiększ na zielonym i czerwonym</OfxPropLabel>
				<OfxPropShortLabel>Zwiększ na zielonym i czerwonym</OfxPropShortLabel>
				<OfxPropLongLabel>Zwiększ na zielonym i czerwonym</OfxPropLongLabel>
				<OfxParamPropHint>Zwiększ na kanale &apos;a&apos; (zielony i czerwony)</OfxParamPropHint>
			</OfxParamTypeDouble>

			<OfxParamTypeDouble name="ColorMagnificationB">
				<OfxPropLabel>Zwiększ na niebieskim i żółtym</OfxPropLabel>
				<OfxPropShortLabel>Zwiększ na niebieskim i żółtym</OfxPropShortLabel>
				<OfxPropLongLabel>Zwiększ na niebieskim i żółtym</OfxPropLongLabel>
				<OfxParamPropHint>Zwiększ na kanale &apos;a&apos; (niebieski i żółty)</OfxParamPropHint>
			</OfxParamTypeDouble>
			
		</OfxImageEffectContext>
	</OfxResourceSet>
</OfxPlugin>
