				<OfxParamPropHint>Wechselt zwischen verschiedenen Stil-Modellen.</OfxParamPropHint>
			</OfxParamTypeChoice>

			<OfxParamTypeDouble name="Mean Red">
				<OfxPropLabel>Durchschnitt Rot</OfxPropLabel>
				<OfxPropShortLabel>Durchschnitt Rot</OfxPropShortLabel>
				<OfxPropLongLabel>Durchschnitt Rot</OfxPropLongLabel>
				<OfxParamPropHint>Fügt den Durchschnittswert für Rot hinzu.</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeDouble name="Mean Green">
				<OfxPropLabel>Durchschnitt Grün</OfxPropLabel>
				<OfxPropShortLabel>Durchschnitt Grün</OfxPropShortLabel>
				<OfxPropLongLabel>Durchschnitt Grün</OfxPropLongLabel>
				<OfxParamPropHint>Fügt den Durchschnittswert für Grün hinzu.</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeDouble name="Mean Blue">
				<OfxPropLabel>Durchschnitt Blau</OfxPropLabel>
				<OfxPropShortLabel>Durchschnitt Blau</OfxPropShortLabel>
				<OfxPropLongLabel>Durchschnitt Blau</OfxPropLongLabel>
				<OfxParamPropHint>Fügt den Durchschnittswert für Blau hinzu.</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeChoice name="Details">
				<OfxParamPropChoiceOption>Gering</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Mittel</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Hoch</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Original</OfxParamPropChoiceOption>
				<OfxPropLabel>Details</OfxPropLabel>
				<OfxPropShortLabel>Details</OfxPropShortLabel>
				<OfxPropLongLabel>Details</OfxPropLongLabel>
				<OfxParamPropHint>Wechselt zwischen verschiedenen Stufen von Detailgenauigkeit.</OfxParamPropHint>
