				<OfxParamPropHint>Alterna entre os diferentes modelos de estilo.</OfxParamPropHint>
			</OfxParamTypeChoice>

			<OfxParamTypeDouble name="Mean Red">
				<OfxPropLabel>Média do vermelho</OfxPropLabel>
				<OfxPropShortLabel>Média do vermelho</OfxPropShortLabel>
				<OfxPropLongLabel>Média do vermelho</OfxPropLongLabel>
				<OfxParamPropHint>Adiciona o valor médio do vermelho.</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeDouble name="Mean Green">
				<OfxPropLabel>Média do verde</OfxPropLabel>
				<OfxPropShortLabel>Média do verde</OfxPropShortLabel>
				<OfxPropLongLabel>Média do verde</OfxPropLongLabel>
				<OfxParamPropHint>Adiciona o valor médio do verde.</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeDouble name="Mean Blue">
				<OfxPropLabel>Média do azul</OfxPropLabel>
				<OfxPropShortLabel>Média do azul</OfxPropShortLabel>
				<OfxPropLongLabel>Média do azul</OfxPropLongLabel>
				<OfxParamPropHint>Adiciona o valor médio do azul</OfxParamPropHint>
			</OfxParamTypeDouble>
			
			<OfxParamTypeChoice name="Details">
				<OfxParamPropChoiceOption>Baixo</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Médio</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Alto</OfxParamPropChoiceOption>
				<OfxParamPropChoiceOption>Original</OfxParamPropChoiceOption>
				<OfxPropLabel>Detalhes</OfxPropLabel>
				<OfxPropShortLabel>Detalhes</OfxPropShortLabel>
				<OfxPropLongLabel>Detalhes</OfxPropLongLabel>
